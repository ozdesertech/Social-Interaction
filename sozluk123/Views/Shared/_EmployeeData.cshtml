@model IEnumerable<sozluk123.Models.entry_yazar>

<div>
    <h4>entry_yazar</h4>
    <hr />
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.comment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.kayit_tarih)
            </th>
           
           
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.comment)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.kayit_tarih)
                </td>
                
            </tr>
        }

    </table>
    </div>